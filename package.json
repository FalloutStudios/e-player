{
    "name": "e-player",
    "description": "Discord music player",
    "author": "FalloutStudios",
    "license": "GPL-3.0",
    "scripts": {
        "clean": "npx rimraf modules",
        "build": "yarn run clean && yarn exec tsc",
        "test": "yarn run build && npx rimraf config/e-player && yarn run prisma:push && npx reciple -D -c reciple.test.yml",
        "start": "yarn exec reciple",
        "prisma:format": "npx prisma format",
        "prisma:generate": "npx prisma generate",
        "prisma:push": "yarn run prisma:format && npx prisma db push"
    },
    "dependencies": {
        "@discordjs/opus": "^0.8.0",
        "@ghextercortes/djs-pagination": "^3.0.0",
        "@prisma/client": "^4.3.1",
        "discord-player": "^5.3.1",
        "discord.js": "^14.3.0",
        "fallout-utility": "^1.4.6",
        "genius-lyrics": "^4.4.0",
        "libsodium-wrappers": "^0.7.10",
        "ms": "^2.1.3",
        "opusscript": "^0.0.8",
        "reciple": "^5.4.4",
        "rimraf": "^3.0.2",
        "sodium": "^3.0.2",
        "tweetnacl": "^1.0.3",
        "yaml": "^2.1.1"
    },
    "devDependencies": {
        "@types/ms": "^0.7.31",
        "@types/node": "^18.7.17",
        "prisma": "^4.3.1",
        "typescript": "^4.8.3"
    }
}
